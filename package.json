{
  "name": "@fatesigner/stylelint-config",
  "version": "0.0.0",
  "description": "The recommended shareable config for stylelint.",
  "license": "MIT",
  "keywords": [
    "stylelint",
    "config"
  ],
  "author": {
    "name": "Jeremy Tang",
    "email": "fatesigner@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fatesigner/stylelint-config.git"
  },
  "bugs": {
    "url": "https://github.com/fatesigner/stylelint-config/issues"
  },
  "homepage": "https://github.com/fatesigner/stylelint-config#readme",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "clean": "gulp clean --gulpfile build/gulpfile.js",
    "build": "gulp build --gulpfile build/gulpfile.js",
    "release": "semantic-release",
    "update": "ncu -u --target minor",
    "prepare": "npx husky install",
    "commit": "npx git-cz",
    "lint": "lint-staged"
  },
  "prettier": "@fatesigner/eslint-config/.prettierrc",
  "peerDependencies": {
    "stylelint": "13.x",
    "stylelint-config-css-modules": "2.x",
    "stylelint-config-recess-order": "2.x",
    "stylelint-config-standard": "22.x",
    "stylelint-order": "4.x"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/eslint-parser": "7.14.7",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@commitlint/cli": "12.1.4",
    "@commitlint/config-conventional": "12.1.4",
    "@fatesigner/cz-customizable": "6.3.0",
    "@fatesigner/eslint-config": "2.0.2",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@types/gulp": "4.0.8",
    "@types/node": "16.0.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "commitizen": "4.2.4",
    "cz-customizable": "6.3.0",
    "eslint": "7.30.0",
    "gulp": "4.0.2",
    "husky": "7.0.0",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "require-dir": "1.2.0",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.4",
    "travis": "0.1.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@fatesigner/cz-customizable"
    }
  }
}
